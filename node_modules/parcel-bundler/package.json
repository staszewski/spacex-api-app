    "prepublish": "yarn build",
    "report-coverage": "nyc report --reporter=lcov && codecov",
    "test": "cross-env NODE_ENV=test mocha",
    "test-ci": "yarn test-coverage && yarn report-coverage",
    "test-coverage": "rm -rf coverage/* && nyc yarn test"
